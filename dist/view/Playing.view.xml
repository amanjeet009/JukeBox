<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
xmlns:control="control" xmlns:jb="a.abc.control" controllerName="a.abc.controller.Playing">
	<App>
		<pages>
			<Page class="page" title="{i18n>title3}" showNavButton="true" navButtonPress="onNavBack" navButtonText="Back">
				<content id="cont" >
			
					<Image width="180px" height="180px" id="image1" src="{i18n>Page3Image}"/>
					
					<ObjectListItem type="Inactive" title="{i18n>column0}: {Songs>SongName}" number="{i18n>column4}: {Songs>Duration}" intro="{i18n>column1}: {Songs>ComposedBy}" activeIcon="sap-icon://accept" class="list">
					
					    <attributes>
					
					        <ObjectAttribute text="{i18n>column3}: {Songs>Label}" id="attribute0"/>
					
					    </attributes>
					
					    <firstStatus>
					
					        <ObjectStatus text="{i18n>column2}: {Songs>LyricsBy}" id="status0"/>
							
					    </firstStatus>
					
					 </ObjectListItem>
					
				   </content>
				<jb:Ccontrol src="{Songs>Path}"></jb:Ccontrol>
			</Page>
		</pages>
	</App>
</mvc:View>